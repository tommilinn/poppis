{
  "name": "nextjs",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "migrate:dev": "npx dotenv -e .env.development.local -- prisma migrate dev",
    "db:push": "npx dotenv -e .env.development.local -- prisma db push",
    "migrate:reset": "npx dotenv -e .env.development.local -- prisma migrate reset",
    "db:seed": "npx dotenv -e .env.development.local -- prisma db seed",
    "prisma:generate": "npx dotenv -e .env.development.local -- prisma generate",
    "prisma:studio": "npx dotenv -e .env.development.local -- prisma studio",
    "postinstall": "prisma generate",
    "prisma:seed": "ts-node prisma/seed_achievement.mjs",
    "prisma:delete:achi": "ts-node prisma/delete_data.mjs",
    "prisma:delete:profile": "ts-node prisma/delete_data_profile.mjs",
    "prisma:clear": "npx prisma db push --force-reset"
  },
  "dependencies": {
    "@prisma/client": "^6.1.0",
    "@radix-ui/react-dialog": "^1.1.4",
    "@radix-ui/react-navigation-menu": "^1.2.3",
    "@radix-ui/react-slot": "^1.2.0",
    "@tanstack/react-query": "^5.62.11",
    "@tanstack/react-query-devtools": "^5.62.11",
    "@vercel/postgres": "^0.10.0",
    "bcrypt": "^5.1.1",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "dotenv-cli": "^8.0.0",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.469.0",
    "next": "^15.1.3",
    "next-auth": "^4.24.11",
    "react": "^19",
    "react-dom": "^19",
    "tailwind-merge": "^2.6.0",
    "tailwindcss": "^4.1.3",
    "tailwindcss-animate": "^1.0.7",
    "tw-animate-css": "^1.2.5"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.3",
    "@types/bcrypt": "^5.0.2",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/node": "^22",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.1.3",
    "postcss": "^8.5.3",
    "prisma": "^6.1.0",
    "ts-node": "^10.9.2",
    "typescript": "^5"
  }
}
